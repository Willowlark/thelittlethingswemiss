---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ site.name | xml_escape }} - Cosplay Photography</title>
		<description>Gallery Posts of Cosplay Work</description>
		<link>{{ site.url }}</link>
		<atom:link href="{{ page.path }}" rel="self" type="application/rss+xml" />
		
        <lastBuildDate>
            {% for post in site.tags.rss limit:1 %}
                    {{ post.date | date_to_rfc822 }}
            {% endfor %}
        </lastBuildDate>

		{% for post in site.tags.rss limit:20 %}
        {% if post.tags contains "cosplay" %}
			<item>
                <title>{{post.title}}</title>
				<description></description>
				<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
				<link>{{site.url}}{{ post.url }}</link>
				<guid isPermaLink="true">{{site.url}}{{ post.url }}</guid>
			</item>
        {% endif %}
		{% endfor %}
	</channel>
</rss>